/* This css component for topbars aand everything in it  */
.topbar {
  display: flex;
  border: 1px solid grey;
  border-radius: 10px;
}

.searchbar {
  display: flex;
  border: 1px solid grey;
  border-radius: 10px;
  padding: 5px;
}

.searchbarinput {
  height: 100%;
  width: 100%;
  border: none;
  outline: none;
}
.searchbardiv {
  display: flex;
  align-items: center;
  padding: 10px;
  cursor: pointer;
}

.signlikecart {
  align-items: center;
  display: flex;
  gap: 20px;
}

.btnhead {
  border: none;
  outline: none;
  background-color: white;
  cursor: pointer;
  font-size: 19px;
}
